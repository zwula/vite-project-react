{
  "name": "vite-project-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "commit": "git-cz",
    "preview": "vite preview",
    "preinstall": "node ./preinstall.js",
    "lint": "eslint . --ext ts,tsx,js,jsx,cjs --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx,js,jsx,cjs --fix --max-warnings 0",
    "style": "stylelint **/*.{css,less}",
    "style:fix": "stylelint **/*.{css,less} --fix",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{js,jsx,cjs,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{css,less}": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitejs/plugin-react": "^4.0.3",
    "cz-git": "^1.7.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.0",
    "less": "^4.2.0",
    "lint-staged": "^14.0.1",
    "path": "^0.12.7",
    "postcss-less": "^6.0.0",
    "prettier": "^3.0.2",
    "stylelint": "^15.10.3",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-less": "^1.0.8",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}